<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Tampilan Awal</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  <style>
    /* Custom CSS */
    body {
      padding-top: 4.5rem;
    }
  </style>
</head>
<body>
  <nav class="navbar navbar-expand-lg navbar-light bg-light fixed-top">
    <div class="container">
      <a class="navbar-brand" href="#">Navbar</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav ms-auto">
          <a class="nav-link active" aria-current="page" href="#">Home</a>
          <a class="nav-link" href="/mahasiswa">Mahasiswa</a>
          <a class="nav-link" href="/pendidikan">Pendidikan</a>
          <a class="nav-link" href="/keahlian">Keahlian</a>
          <a class="nav-link" href="/kategori">Kategori</a>
        </div>
      </div>
    </div>
  </nav>

  <div class="container mt-5">
    <% if (messages.success) { %>
      <div class="alert alert-info" role="alert">
        <%- messages.success %>
      </div>
    <% } %>
    <div class="row">
      <div class="col">
        <a href="/alat_tangkap/create" class="btn btn-sm btn-success mb-3">Tambah</a>
        <table class="table">
          <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">alat_tangkap</th>
              <th colspan="2" scope="col">Aksi</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i = 0; i < data.length; i++){ %>
              <tr>
                <th scope="row"><%= (i+1) %></th>
                <td><%= data[i].alat_tangkap %></td>
                <td>
                  <a href="/alat_tangkap/edit/<%= data[i].id_alat_tangkap %>" class="btn btn-sm btn-warning">Edit</a>
                  <a href="/alat_tangkap/delete/<%= data[i].id_alat_tangkap %>" class="btn btn-sm btn-danger">Delete</a>
                </td>
              </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>
